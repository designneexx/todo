{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "lib": [
      "esnext",
      "dom.iterable",
      "dom"
    ],
    "module": "esnext",
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true
  }
}